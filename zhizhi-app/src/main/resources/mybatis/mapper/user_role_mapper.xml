<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xu.infrastructure.persistent.dao.IUserRoleDao">

    <resultMap id="BaseResultMap" type="cn.xu.infrastructure.persistent.po.UserRole">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="userId" column="user_id" jdbcType="BIGINT"/>
            <result property="roleId" column="role_id" jdbcType="BIGINT"/>
    </resultMap>

    <insert id="insert" parameterType="cn.xu.infrastructure.persistent.po.UserRole">
        INSERT INTO user_role (user_id, role_id) VALUES (#{userId}, #{roleId})
    </insert>

</mapper>
